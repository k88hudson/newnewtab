{
  "name": "newtab",
  "version": "0.0.0",
  "description": "Remote new tab (third version)",
  "main": "src/main.js",
  "scripts": {
    "start": "npm run clean && npm run watch:js & npm run watch:css",
    "build": "npm run clean && npm run build:js & npm run build:css",
    "test": "karma start",
    "test:watch": "karma start --no-single-run --browsers ChromeCanary",
    "clean": "rm -rf ./www",
    "build:js": "NODE_ENV=production webpack -p --optimize-dedupe",
    "build:css": "node-sass src/main.scss -o www",
    "watch:css": "npm run build:css && npm run build:css -- --source-map www/main.css.map -w -r",
    "watch:js": "webpack-dev-server --hot --port=1944 --output-public-path www"
  },
  "author": "k88hudson",
  "license": "MPL-2.0",
  "devDependencies": {
    "babel": "^6.3.13",
    "babel-core": "^6.3.17",
    "babel-loader": "^6.2.0",
    "babel-plugin-transform-es2015-destructuring": "^6.3.15",
    "babel-plugin-transform-es2015-parameters": "^6.3.18",
    "babel-plugin-transform-object-rest-spread": "^6.3.13",
    "babel-plugin-transform-strict-mode": "^6.3.13",
    "babel-polyfill": "^6.3.14",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-react": "^6.3.13",
    "chai": "^3.4.1",
    "eslint": "^1.10.2",
    "eslint-config-airbnb": "^1.0.2",
    "eslint-loader": "^1.1.1",
    "eslint-plugin-react": "^3.10.0",
    "inject-loader": "^2.0.1",
    "karma": "^0.13.15",
    "karma-chrome-launcher": "^0.2.2",
    "karma-firefox-launcher": "^0.1.7",
    "karma-mocha": "^0.2.1",
    "karma-mocha-reporter": "^1.1.3",
    "karma-sourcemap-loader": "^0.3.6",
    "karma-webpack": "^1.7.0",
    "mocha": "^2.3.4",
    "mozilla-download": "^1.1.1",
    "nock": "^3.5.0",
    "node-sass": "^3.4.2",
    "react-addons-test-utils": "0.14.5",
    "redux-mock-store": "0.0.3",
    "webpack": "^1.12.9",
    "webpack-dev-server": "^1.14.0",
    "webpack-notifier": "^1.2.1"
  },
  "dependencies": {
    "classnames": "2.2.1",
    "react": "0.14.5",
    "react-dom": "0.14.5",
    "react-redux": "4.0.1",
    "redux": "3.0.5",
    "redux-logger": "2.2.1",
    "redux-thunk": "1.0.2"
  }
}
